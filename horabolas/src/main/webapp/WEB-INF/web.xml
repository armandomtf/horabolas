    <servlet-mapping>
        <servlet-name>UsuarioServletD</servlet-name>
        <url-pattern>/UsuarioServletD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioAdmServletSA</servlet-name>
        <url-pattern>/UsuarioAdmServletSA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioAdmServletLogin</servlet-name>
        <url-pattern>/UsuarioAdmServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServletAlterar</servlet-name>
        <url-pattern>/UsuarioServletAlterar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletQuadra</servlet-name>
        <url-pattern>/ServletQuadra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManipularImagem</servlet-name>
        <url-pattern>/ManipularImagem</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
