    <servlet>
        <servlet-name>ManipularImagem</servlet-name>
        <servlet-class>com.br.horabolas.servlet.ManipularImagem</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioServletSA</servlet-name>
        <url-pattern>/UsuarioServletSA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServletLogin</servlet-name>
        <url-pattern>/UsuarioServletLogin</url-pattern>
